{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "typeRoots": [
      "./node_modules/@types",
      "./@types"
    ]
  }
}
